{#
Surcharge this template in app/Resources/TaphaBlogBundle/views/base.layout.html.twig if needed
#}
{% extends '::base.html.twig' %}

{% block blog_title %}
    {{ 'tapha_blog.blog_title'|parameter() }}
{% endblock %}
    
{% block body %}
    {% block blog_body %}{% endblock %} 
{% endblock %}    

{% block stylesheets %}
  {{ parent() }}
  <link rel="stylesheet" href="{{ asset('bundles/taphablog/css/default.css') }}" type="text/css" media="all" />
  <link rel="stylesheet" href="{{ asset('bundles/taphablog/css/blog.css') }}" type="text/css" media="all" />
  <link rel="stylesheet" href="{{ asset('bundles/taphablog/css/colours.css') }}" type="text/css" media="all" />
{% endblock %}
{% block javascripts %}
  {{ parent() }}
{% endblock %}